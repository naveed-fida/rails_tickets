<div class="projects">
  <h1>Projects</h1>
  <ul>
  <% @projects.each do |project| %>
    <li>
      <h3><%= link_to project.name, project_path(project) %></h3>
      <p><%= project.description %></p>
    </li>
  <% end %>
  </ul>
</div>

<div class="actions">
  <%= link_to 'New Project', new_project_path %>
</div>
